    <div class="album py-5 bg-light">
        <div class="container">
            <div class="row">
                <div class="col-md-3 project-detail-sb">
                    <div class="project-sidebar-links">
                        <h2>Links</h2>
                        <div class="project-sidebar-links">
                            <a href="/projects/demo/{{project.alias}}" class="btn btn-primary">Live Demo</a>
                        </div>
                        <div class="project-sidebar-links">
                            <a href="#" class="btn btn-secondary">Github</a>
                        </div>
                    </div>

                    <hr>

                    <div class="project-sidebar-links">
                        <h2>Tags</h2>
                             {{#each project.tags}}
                                <a href="#" class="project-sidebar-links"><span class="badge badge-{{this.class}}">{{this.name}}</span></a>
                             {{/each}}
                    </div>
                    <hr>


                    <div class="project-sidebar-links">
                        <h2>Related Projects</h2>
                             {{#each project.relatedProjects}}
                        
                            <div class="project-sidebar-links">
                                <a href="#">{{this.name}}</a>
                            </div>
                             {{/each}}
                    </div>
                    <hr>
                    <div class="project-sidebar-links">
                        <h2>Resources</h2>
                        <div class="project-sidebar-links">
                            <div class="project-sidebar-links">
                                <a href="#">JS Console</a>
                            </div>
                            <div class="project-sidebar-links">
                                <a href="#">Bouncing Balls</a>
                            </div>
                            <div class="project-sidebar-links">
                                <a href="#">Image Gallery</a>
                            </div>
                            <div class="project-sidebar-links">
                                <a href="#">Matrix Effect</a>
                            </div>
                        </div>
                    </div>


                </div>
                <div class="col-md-9">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item "><a href="/">Home</a></li>
                            <li class="breadcrumb-item "><a href="#">Projects</a></li>
                            <li class="breadcrumb-item " aria-current="page">{{project.alias}}</li>
                        </ol>
                    </nav>

                    <div>
                        <h2>{{project.name}}</h2>
                        <p>{{project.description}}</p>
                    </div>
                </div>
            </div>

        </div>
    </div>
